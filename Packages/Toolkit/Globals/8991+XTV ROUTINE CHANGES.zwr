^XTV(8991,9,1,1,1,82,0)=" .S PRISEC=$P($G(PXBSAM(LINE)),""^"",4) S:PRISEC[""PRI"" FPRI=0"
^XTV(8991,9,1,1,1,83,0)=" S POV=Y(0)"
