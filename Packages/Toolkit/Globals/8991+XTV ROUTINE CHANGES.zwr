^XTV(8991,9,1,1,1,54,0)=" ;"
^XTV(8991,9,1,1,1,55,0)="LI ;--------If picked a line number"
^XTV(8991,9,1,1,1,56,0)=" I (DATA>0)&(DATA<(PXBCNT+1))&($L(DATA)'>$L(PXBCNT)) S XFLAG=1 D REVPOV^PXBCC(DATA) S SELINE=DATA D"
^XTV(8991,9,1,1,1,57,0)=" .F I=1:1:$L(DATA) W IOCUB,IOECH"
^XTV(8991,9,1,1,1,58,0)=" .S PRISEC=$P($G(PXBSAM(DATA)),""^"",4) S:PRISEC[""PRI"" FPRI=0"
^XTV(8991,9,1,1,1,59,0)=" .S DATA=$P($G(PXBSAM(DATA)),""^"",1)"
^XTV(8991,9,1,1,1,60,0)=" I $D(XFLAG),XFLAG=1 S (Y,EDATA)=DATA G PFIN"
^XTV(8991,9,1,1,1,61,0)="LI1 ;"
^XTV(8991,9,1,1,1,62,0)=" ;--------If POV is already in the file"
