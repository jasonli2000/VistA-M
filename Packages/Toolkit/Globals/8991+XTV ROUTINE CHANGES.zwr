^XTV(8991,9,1,1,1,6,0)=" ;"
^XTV(8991,9,1,1,1,7,0)="POV ;--DIAGNOSIS"
^XTV(8991,9,1,1,1,8,0)=" I $D(PXBNPOVL) D LOC^PXBCC(2,0) W IOUON,""Previous Entry:   "",$G(PXBNPOVL(1)) F I=1:1:10 W "" """
^XTV(8991,9,1,1,1,9,0)=" W IOUOFF"
