^XTV(8991,9,1,1,0)="2971007.1008"
^XTV(8991,9,1,1,1,0)="^8991.11^100^100"
^XTV(8991,9,1,1,1,1,0)="PXBPPOV ;ISL/JVS - PROMPT POV ;10/7/97  10:07"
^XTV(8991,9,1,1,1,2,0)=" ;;1.0;PCE PATIENT CARE ENCOUNTER;**11,28**;Aug 12, 1996"
