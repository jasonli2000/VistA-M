^XTV(8991,9,1,1,1,37,0)=" ;-----------------"
^XTV(8991,9,1,1,1,38,0)=" I DATA=""^^"" S PXBEXIT=0 G POVX"
^XTV(8991,9,1,1,1,39,0)=" ;---I Prompt can jump to others put symbols in here"
^XTV(8991,9,1,1,1,40,0)=" I DATA[""^P"" G POVX"
^XTV(8991,9,1,1,1,41,0)=" ;------PXBDPOV=DEFAULT POV---"
^XTV(8991,9,1,1,1,42,0)=" I DATA="""",$D(PXBDPOV) S DATA=$P($G(PXBDPOV),""--"",1)"
^XTV(8991,9,1,1,1,43,0)=" I DATA="""",'$D(PXBDPOV) S PXBUT=1,PXBSPL="""",LEAVE=1 G POVX"
^XTV(8991,9,1,1,1,44,0)=" ;"
^XTV(8991,9,1,1,1,45,0)=" I PXBCNT>10&((DATA=""+"")!(DATA=""-"")) D DPOV4^PXBDPOV(DATA) G P"
^XTV(8991,9,1,1,1,46,0)=" ;"
^XTV(8991,9,1,1,1,47,0)="M ;--------IF Multiple entries have been entered"
^XTV(8991,9,1,1,1,48,0)=" D ADDM^PXBPPOV1"
^XTV(8991,9,1,1,1,49,0)=" I $G(NF) G P1"
^XTV(8991,9,1,1,1,50,0)=" ;"
