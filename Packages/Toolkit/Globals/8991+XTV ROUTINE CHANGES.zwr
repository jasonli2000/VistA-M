^XTV(8991,7,1,1,1,68,0)=" ;; "
^XTV(8991,7,1,1,1,69,0)=" ;; "
^XTV(8991,7,1,1,1,70,0)=" ;;                           _____________________________________"
^XTV(8991,7,1,1,1,71,0)=" ;;                           Signature of EEO Counselor       Date"
^XTV(8991,7,1,1,1,72,0)=" ;; "
^XTV(8991,7,1,1,1,73,0)=" ;;***"
