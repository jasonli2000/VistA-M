^XTV(8991,9,1,1,1,64,0)=" .I PXBCNT>10 D DPOV4^PXBDPOV($O(PXBKY(DATA,0)))"
^XTV(8991,9,1,1,1,65,0)=" .K Q D TIMES^PXBUTL(DATA)"
^XTV(8991,9,1,1,1,66,0)=" .I Q=1 S LINE=$O(PXBKY(DATA,0)) S XFLAG=1 D REVPOV^PXBCC(LINE) S PRISEC=$P($G(PXBSAM(LINE)),""^"",2) S:PRISEC[""PRI"" FPRI=0"
^XTV(8991,9,1,1,1,67,0)=" .I Q>1 S NLINE=0 F  S NLINE=$O(Q(NLINE)) Q:NLINE=""""  D REVPOV^PXBCC(NLINE)"
^XTV(8991,9,1,1,1,68,0)=" I $D(Q),Q>1 D WHICH^PXBPWCH G LI"
^XTV(8991,9,1,1,1,69,0)=" I $D(XFLAG),XFLAG=1 S Y=DATA G PFIN"
^XTV(8991,9,1,1,1,70,0)=" ;"
^XTV(8991,9,1,1,1,71,0)=" ;--------Need to do a DIC lookup on data"
^XTV(8991,9,1,1,1,72,0)=" I DATA'=""??"" D:DATA=""?"" EN1^PXBHLP0(""PXB"",""POV"",1,"""",1) G:DATA=""^P"" P1 I DATA=""?"" G P"
^XTV(8991,9,1,1,1,73,0)=" I DATA=""??"" S DOUBLEQQ=1 D EN1^PXBHLP0(""PXB"",""POV"","""",1,2) S:$L(DATA,""^"")>1 (Y,DATA,EDATA)=$P($P(DATA,""^"",2),""--"",1) G:Y>1 PFIN G:Y?1A1.NP PFIN I DATA<1 S DATA=""^P"" G P1"
^XTV(8991,9,1,1,1,74,0)=" ;"
^XTV(8991,9,1,1,1,75,0)=" ;--If a ""?"" is NOT entered during lookup"
^XTV(8991,9,1,1,1,76,0)=" S (VAL,Y)=$$DOUBLE1^PXBGPOV2(WHAT) I Y<1 S DATA=""^P"" G P1"
^XTV(8991,9,1,1,1,77,0)=" S (X,DATA,EDATA)=$P(VAL,""^"",2),DIC=80,DIC(0)=""MZ"" D ^DIC"
^XTV(8991,9,1,1,1,78,0)=" ;"
^XTV(8991,9,1,1,1,79,0)=" ;--If Y is good and already in file..."
